<%= form_for @title,:url => {:action => :create} do |f| %>
  <div class="form-group">
    <%= f.label :name %>
    <%= f.text_field :name , class:'form-control' %>
    <%= f.label :about %>
    <%= f.text_field :about , class:'form-control' %>
    <%= f.submit class:'btn btn-default' %>
  </div>
<% end %>
